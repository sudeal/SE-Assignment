<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    
</head>
<body>
  
    <h2>Categories</h2>
    <ul>
        <% categories.forEach(category => { %>
            <li>
                <a href="/search?category=<%= category.toLowerCase() %>"><%= category %></a>
            </li>
        <% }); %>
    </ul>
</body>
</html>
